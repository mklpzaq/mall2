<mapper namespace="com.test.mall2.member.service.MemberMapper">
 <insert id="insertMember" parameterType="com.test.mall2.member.service.Member">
	 INSERT INTO member(
		 member_id 
		 ,member_pw
		 ) VALUSE (
			 #{memberId}
			 ,#{memberPw}
	 )
